ent":{"action":"change_page","value":"11"}},{"text":"\\n-New items.","bold":false,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 12."}]},"clickEvent":{"action":"change_page","value":"12"}},{"text":"\\n-Wiki & Showcase.","bold":false,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 14."}]},"clickEvent":{"action":"change_page","value":"13"}},{"text":"\\nUse your recipe book!","color":"dark_red","bold":false}]','[{"text":"----Settings----","bold":true},{"text":"\\nFishing Trophies","bold":false,"underlined":true},{"text":"\\nEnable:","bold":false,"italic":true},{"text":"  [✔]","color":"green","bold":false,"hoverEvent":{"action":"show_text","contents":[{"text":"Click to enable."}]},"clickEvent":{"action":"run_command","value":"/function tsurvival:trophies/enable"}},{"text":"\\nDisable:","bold":false,"italic":true},{"text":" [✖]","color":"dark_red","bold":false,"hoverEvent":{"action":"show_text","contents":[{"text":"Click to disable."}]},"clickEvent":{"action":"run_command","value":"/function tsurvival:trophies/disable"}}]','[{"text":"----Premise----","bold":true},{"text":"\\nThis datapack aims to do a few things:","color":"dark_red","bold":false},{"text":"\\n1)Increase difficulty.","bold":false},{"text":"\\n2)Introduce new, integrated mechanics.","bold":false},{"text":"\\n3)Improve QOL...","bold":false},{"text":"\\n all while not disrupting sandbox vanilla gameplay!","bold":false},{"text":"\\nWelcome to:","italic":true},{"text":"\\nTRUE SURVIVAL!","bold":true},{"text":"\\nVisit wiki for more info.","bold":false,"italic":true,"underlined":true,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 13."}]},"clickEvent":{"action":"change_page","value":"13"}}]','[{"text":"----Healing----","bold":true,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 12."}]},"clickEvent":{"action":"change_page","value":"12"}},{"text":"\\nTrue survival has natural healing disabled. Be resourceful!","color":"dark_red","bold":false,"italic":true},{"text":"\\nThrow a log onto a campfire to slowly regenerate 3 hearts.","bold":false},{"text":"\\nHealing salves will also be useful.","bold":false},{"text":"\\nClick for more info.","color":"dark_blue","bold":false,"italic":true,"underlined":true}]','[{"text":"----Lighting----","bold":true},{"text":"\\nTorches cannot be crafted the normal way! Upgrade your lighting to other sources!","color":"dark_red","bold":false,"italic":true},{"text":"\\nPlace sticks onto a campfire to make 1. To make 4, craft a stick and coal/charcoal with grass or other kindling. To make 6, craft a stick with a fire charge.","bold":false}]','[{"text":"----Hunger----","bold":true},{"text":"\\nUpgrade your food or reduce your hunger usage!","color":"dark_red","bold":false,"italic":true},{"text":"\\nThere are many mechanics that will now require hunger like sleeping, special mob attacks, and high-end gear.","bold":false},{"text":"\\nClick for more info.","color":"dark_blue","bold":false,"italic":true,"underlined":true,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 13."}]},"clickEvent":{"action":"change_page","value":"13"}}]','[{"text":"----Iron----","bold":true},{"text":"\\nThere is a 10% chance to acquire an iron nugget from mining stone! Deepslate will drop more.","color":"dark_red","bold":false,"italic":true},{"text":"\\n4 nuggets in diamond shape craft into chains, which craft into chainmail. Golems only give iron if killed manually.","bold":false},{"text":"\\nClick for more info.","color":"dark_blue","bold":false,"italic":true,"underlined":true,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 13."}]},"clickEvent":{"action":"change_page","value":"13"}}]','[{"text":"----Traders----","bold":true,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 9."}]},"clickEvent":{"action":"change_page","value":"9"}},{"text":"\\nThere are 3 new traders!","color":"dark_red","bold":false,"italic":true},{"text":"\\nSpelunker: Spawns underground and helps with mining.","bold":false},{"text":"\\nJockey: Spawns in the nether and helps nether exploration.","bold":false},{"text":"\\nVoyager: Spawns in The End and helps End exploration.","bold":false},{"text":"\\nClick for more info.","color":"dark_blue","bold":false,"italic":true,"underlined":true,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 13."}]},"clickEvent":{"action":"change_page","value":"13"}}]','[{"text":"-----Armor-----","bold":true},{"text":"\\nArmors have sets and reworks!","color":"dark_red","bold":false,"italic":true},{"text":"\\nLeather: Speed and immunity to creeper slowness.","bold":false},{"text":"\\nChainmail: Immunity to zombie bites.","bold":false},{"text":"\\nGold: Haste.","bold":false},{"text":"\\nDiamond: Chestplate uses hunger.","bold":false},{"text":"\\nNetherite: Chestplate uses hunger. Gives fire resistance.","bold":false}]','[{"text":"----Wood----","bold":true},{"text":"\\nWood income now has progression!","color":"dark_red","bold":false,"italic":true},{"text":"\\nFortune on axes will yield growing multiples of planks.","bold":false},{"text":"\\nLumberjack villagers will sell large shipments of logs! To hire one, splash a Vindicator with weakness and rename them: Bunyan.","bold":false},{"text":"\\nClick for more info.","color":"dark_blue","bold":false,"italic":true,"underlined":true,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 13."}]},"clickEvent":{"action":"change_page","value":"13"}}]','[{"text":"----Mobs----","bold":true},{"text":"\\nCreeper: Gives slowness when nearby.","bold":false},{"text":"\\nZombiesX: Apply hunger effects on hit.","bold":false},{"text":"\\nSkeleton: Armored skeletons switch to axes when close by.","bold":false},{"text":"\\nSpider: Strength spiders will hatch spiderlings and lay cobwebs.","bold":false},{"text":"\\nClick for more info.","color":"dark_blue","bold":false,"italic":true,"underlined":true,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 13."}]},"clickEvent":{"action":"change_page","value":"13"}}]','[{"text":"----End----","bold":true},{"text":"\\nMobs drop emeralds! But: they\'re used for progression.","color":"dark_red","bold":false,"italic":true},{"text":"\\nDrop rate depends on difficulty, rarity, and farmability.","bold":false},{"text":"\\nEnder eyes require an emerald.","bold":false},{"text":"\\nEnchantment tables require an emerald block.","bold":false},{"text":"\\nEnder chests require 2 emerald blocks.","bold":false}]','[{"text":"----Items----","bold":true},{"text":"\\nNew items fill holes in vanilla gameplay! Not all are shown here.","color":"dark_red","bold":false,"italic":true},{"text":"\\nMake the custom crafter!","bold":false},{"text":"\\nPlace an item frame on top of a crafting table. Then place an emerald inside the frame. Right click the campfire to use!","bold":false},{"text":"\\nClick for more info.","color":"dark_blue","bold":false,"italic":true,"underlined":true,"hoverEvent":{"action":"show_text","contents":[{"text":"Move to page 13."}]},"clickEvent":{"action":"change_page","value":"13"}}]','[{"text":"----Wiki----","bold":true},{"text":"\\nThis guidebook should get you started, but there are many more things in True Survival! ","color":"dark_red","bold":false,"italic":true},{"text":"\\nMake sure to reference the Wiki and/or the full feature showcase to learn more!","bold":false},{"text":"\\nClick to learn more!","color":"dark_blue","bold":false,"italic":true,"underlined":true,"hoverEvent":{"action":"show_text","contents":[{"text":"Open Wiki webpage."}]},"clickEvent":{"action":"open_url","value":"http://truesurvivalmc.weebly.com/"}}]']}}}
